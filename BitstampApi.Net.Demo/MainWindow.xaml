<Window x:Class="Jojatekok.BitstampAPI.Demo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Bitstamp API .NET Demo"
        Width="300"
        SizeToContent="Height"
        ResizeMode="NoResize">
    <StackPanel Margin="10">
        <TextBlock Text="Last Bitcoin price:" HorizontalAlignment="Center" />
        <TextBlock x:Name="TextBlockPriceLast" Text="Loading..." FontSize="30" HorizontalAlignment="Center" />
    </StackPanel>
</Window>
